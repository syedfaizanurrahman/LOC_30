#include <stdio.h>

int main()
{
    int rows = 5;

    // Loop through each row
    for (int i = rows; i >= 1; i--)
    {
        // Loop through each column in the current row
        for (int j = 1; j <= i; j++)
        {
            printf("* ");
        }
        printf("\n");
    }

    return 0;
}
